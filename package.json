{
	"name": "spiral-bundler",
	"version": "1.0.0",
	"description": "Spiral Creative Group starter kit",
	"repository": "",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "npm run dev",
		"backend": "nodemon --inspect --watch src/server --watch config src/server",
		"frontend": "webpack-dev-server --config config/webpack.dev.js --hot --inline",
		"dev": "npm run img && set NODE_ENV=development&& concurrently \"npm:backend\" \"npm:frontend\"",
		"dev:stats": "set STATS=true&& npm run dev",
		"build": "npm run img && del-cli dist/* && set NODE_ENV=production&& set BABEL_ENV=production&& webpack --config=config/webpack.prod.js --env.NODE_ENV=production&& node config/postscript.js",
		"build:stats": "set STATS=true&& npm run build",
		"img:sprite": "svg-sprite --log=info --symbol --symbol-dest=src/images --symbol-sprite=sprite.svg src/images-src/svg-sprite/*.svg",
		"img:icons": "vsvg -s ./src/images-src/svg-icons -t ./src/images/svg-icons",
		"img:static": "node config/buildImages.js",
		"img": "npm run img:static && npm run img:sprite && npm run img:icons"
	},
	"dependencies": {
		"@babel/runtime": "^7.4.5",
		"@vue/composition-api": "^1.2.4",
